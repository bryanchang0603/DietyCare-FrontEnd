    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:srcCompat="@drawable/ic_baseline_thumb_up_24"
        tools:layout_editor_absoluteX="@dimen/_252sdp"
        tools:layout_editor_absoluteY="@dimen/_270sdp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        app:srcCompat="@drawable/toux"
        tools:layout_editor_absoluteX="@dimen/_31sdp"
        tools:layout_editor_absoluteY="@dimen/_267sdp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        app:srcCompat="@drawable/toux"
        tools:layout_editor_absoluteX="@dimen/_170sdp"
        tools:layout_editor_absoluteY="@dimen/_267sdp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
