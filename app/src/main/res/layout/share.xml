        tools:layout_editor_absoluteX="@dimen/_170sdp"
        tools:layout_editor_absoluteY="@dimen/_320sdp"
        tools:srcCompat="@tools:sample/avatars" />

    <EditText
        android:id="@+id/textArea_information"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_30sdp"
        android:background="@null"
        android:gravity="top|left"
        android:inputType="textMultiLine"
        android:lines="6"
        android:overScrollMode="always"
        android:scrollbarStyle="insideInset"
        android:scrollbars="vertical"
        android:scrollHorizontally="false"
        android:text="Text here..."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#B1AEAE"
        tools:layout_editor_absoluteX="@dimen/_31sdp"
        tools:layout_editor_absoluteY="@dimen/_237sdp" />

    <EditText
        android:id="@+id/textArea_information2"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_30sdp"
        android:background="@null"
        android:gravity="top|left"
        android:inputType="textMultiLine"
        android:lines="6"
        android:overScrollMode="always"
        android:scrollbarStyle="insideInset"
        android:scrollbars="vertical"
        android:scrollHorizontally="false"
        android:text="Text here..."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#B1AEAE"
        tools:layout_editor_absoluteX="@dimen/_170sdp"
        tools:layout_editor_absoluteY="@dimen/_237sdp" />

    <EditText
        android:id="@+id/textArea_information3"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_30sdp"
        android:background="@null"
        android:gravity="top|left"
        android:inputType="textMultiLine"
        android:lines="6"
        android:overScrollMode="always"
        android:scrollbarStyle="insideInset"
        android:scrollbars="vertical"
        android:scrollHorizontally="false"
        android:text="Text here..."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#B1AEAE"
        tools:layout_editor_absoluteX="@dimen/_170sdp"
        tools:layout_editor_absoluteY="@dimen/_420sdp" />

    <EditText
        android:id="@+id/textArea_information4"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_30sdp"
        android:background="@null"
        android:gravity="top|left"
        android:inputType="textMultiLine"
        android:lines="6"
        android:overScrollMode="always"
        android:scrollbarStyle="insideInset"
        android:scrollbars="vertical"
        android:scrollHorizontally="false"
        android:text="Text here..."
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#B1AEAE"
        tools:layout_editor_absoluteX="@dimen/_31sdp"
        tools:layout_editor_absoluteY="@dimen/_420sdp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:srcCompat="@drawable/ic_baseline_thumb_up_24"
        tools:layout_editor_absoluteX="@dimen/_113sdp"
        tools:layout_editor_absoluteY="@dimen/_270sdp" />

    <ImageButton
